    echo "  if [ -z $isTutorInstalled ]; then"
    echo "    echo /"install tutor/""
    echo "    wget -q  https://github.com/ole-vi/tutor-rpi/releases/download/v10.0.10-treehouses.1/tutor"
    echo "    chmod +x tutor"
    echo "    mv tutor /usr/local/bin/"
    echo "  else"
    echo "    echo /"tutor is installed/""
    echo "  fi"
    echo "else"
    echo "  echo /"ERROR: script does not work/""
    echo "exit 1"
    echo "fi"
    echo ""
